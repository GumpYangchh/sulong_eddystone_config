<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gump.sulong.eddystone.config" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EddyStone</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author href="https://github.com/GumpYangchh">
           Gump Yang
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/sulong.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/sulong.png" />
        <icon density="mdpi" src="res/sulong.png" />
        <icon density="hdpi" src="res/sulong.png" />
        <icon density="xhdpi" src="res/sulong.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
